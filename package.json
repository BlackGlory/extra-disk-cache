{
  "name": "extra-disk-cache",
  "version": "0.8.5",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "migrations"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:BlackGlory/extra-disk-cache.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --target es2015 --outDir lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --target es2018 --outDir lib/es2018",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.5.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/better-sqlite3": "^7.6.2",
    "@types/jest": "^29.2.3",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "husky": "^4.3.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-patch": "^2.0.2",
    "typescript": "^4.9.3",
    "typescript-transform-paths": "^3.4.4"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.16",
    "@blackglory/prelude": "^0.1.8",
    "@blackglory/types": "^1.4.0",
    "better-sqlite3": "^8.0.0",
    "extra-filesystem": "^0.4.7",
    "extra-lazy": "^1.3.1",
    "extra-promise": "^4.3.0",
    "extra-timers": "^0.2.5",
    "iterable-operator": "^2.5.0",
    "migrations-file": "^0.2.5"
  }
