{
  "name": "extra-disk-cache",
  "version": "0.3.2",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "migrations"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:BlackGlory/extra-disk-cache.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --target es2015 --outDir lib/es2015",
    "build:es2015:copy-d-ts": "cp src/*.d.ts lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --target es2018 --outDir lib/es2018",
    "build:es2018:copy-d-ts": "cp src/*.d.ts lib/es2018",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.1.21",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/better-sqlite3": "^7.4.1",
    "@types/jest": "^27.0.3",
    "@types/levelup": "^4.3.3",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "husky": "^4.3.0",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.1",
    "ts-patch": "^2.0.1",
    "typescript": "^4.5.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.7",
    "@blackglory/types": "^0.6.1",
    "better-sqlite3": "^7.4.5",
    "extra-filesystem": "^0.3.14",
    "extra-promise": "^0.19.7",
    "extra-timers": "^0.2.4",
    "level-rocksdb": "^5.0.0",
    "migrations-file": "^0.1.10",
    "pkg-dir": "5"
  }
}
